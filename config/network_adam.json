{
  "network": {
      "layers": [
          {
              "type": "input",
              "units": 30,
              "activation": "linear"
          },
          {
              "type": "hidden",
              "units": 32,
              "activation": "relu",
              "weights_initializer": "xavierUniform"
          },
          {
              "type": "hidden",
              "units": 16,
              "activation": "leakyRelu",
              "weights_initializer": "xavierUniform"
          },
          {
              "type": "output",
              "units": 2,
              "activation": "softmax",
              "weights_initializer": "xavierUniform"
          }
      ]
  },
  "training": {
      "loss": "categoricalCrossentropy",
      "optimizer": "adam",
      "optimizer_params": {
          "learning_rate": 0.001,
          "beta1": 0.9,
          "beta2": 0.999,
          "epsilon": 1e-8
      },
      "batch_size": 32,
      "epochs": 200
  }
}
