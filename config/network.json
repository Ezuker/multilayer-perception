{
    "network": {
      "layers": [
        {
          "type": "input",
          "units": 30,
          "activation": "sigmoid"
        },
        {
          "type": "hidden",
          "units": 128,
          "activation": "sigmoid",
          "weights_initializer": "heUniform"
        },
        {
          "type": "hidden",
          "units": 64,
          "activation": "sigmoid",
          "weights_initializer": "heUniform"
        },
        {
          "type": "output",
          "units": 2,
          "activation": "softmax",
          "weights_initializer": "heUniform"
        }
      ]
    },
    "training": {
      "loss": "categoricalCrossentropy",
      "learning_rate": 0.01,
      "batch_size": 32,
      "epochs": 100
    }
  }