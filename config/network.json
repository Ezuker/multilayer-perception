{
  "network": {
      "layers": [
          {
              "type": "input",
              "units": 30,
              "activation": "linear"
          },
          {
              "type": "hidden",
              "units": 32,
              "activation": "sigmoid",
              "weights_initializer": "heUniform"
          },
          {
                "type": "dropout",
                "units": 32,
                "dropout": 0.2
          },
          {
              "type": "hidden",
              "units": 16,
              "activation": "relu",
              "weights_initializer": "heUniform"
          },
          {
              "type": "output",
              "units": 2,
              "activation": "softmax",
              "weights_initializer": "xavierUniform"
          }
      ]
  },
  "training": {
      "loss": "binary_crossentropy",
      "learning_rate": 0.01,
      "batch_size": 16,
      "epochs": 700
  }
}